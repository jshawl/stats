<h1>Listing clicks</h1>

<table>
  <thead>
    <tr>
      <th>Stat</th>
      <th>Referer</th>
      <th >ip</th>
    </tr>
  </thead>

  <tbody>
    <% @clicks.each do |click| %>
      <tr>
        <td><%= click.stat %></td>
        <td><%= click.referrer %></td>
        <td><%= click.ip %></td>
        <td><%= click.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Click', new_click_path %>
